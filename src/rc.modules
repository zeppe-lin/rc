#!/bin/sh
#
# /etc/rc.modules: module initialization script
#

# Refresh module list for correct loading order.
if [ -x /sbin/depmod ]; then
	/sbin/depmod -a
fi

# Load modules from configuration directories.
if [ -x /sbin/modules-load ]; then
	/sbin/modules-load -v
fi

